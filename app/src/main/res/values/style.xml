<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="simple_design">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0.08</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="manage_your">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#535353</item>
    </style>
    <style name="Widget.BottomNavigationView" parent="Widget.Design.BottomNavigationView">
        <item name="fontFamily">@font/clearsans_regular</item>
        <item name="android:textSize">@dimen/_9ssp</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorControlNormal">@color/colorPrimary</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
    </style>

</resources>